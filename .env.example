# Statuspage API Configuration
# These are now server-side only (no NEXT_PUBLIC_ prefix)
# API keys are never exposed to the browser
STATUSPAGE_API_KEY=your_statuspage_api_key_here
STATUSPAGE_PAGE_ID=your_statuspage_page_id_here

# Optional: Password protection for the app
# If set, users will need to enter this password to access the dashboard
BACKUP_PASSWORD=your_optional_password_here

# Optional: AI Gateway API key (for OpenAI/Anthropic via Vercel AI Gateway)
# Enables AI-powered message improvement
AI_GATEWAY_API_KEY=your_ai_gateway_key_here
